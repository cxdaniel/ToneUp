{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter: Debug (Current Device)",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main.dart",
      "flutterMode": "debug",
      "console": "terminal"
    },
    {
      "name": "Flutter: Debug (iOS Simulator)",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main.dart",
      "flutterMode": "debug",
      "deviceId": "ios",
      "console": "terminal"
    },
    {
      "name": "Flutter: Debug (Android Emulator)",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main.dart",
      "flutterMode": "debug",
      "deviceId": "emulator-5554",
      "console": "terminal"
    },
    {
      "name": "Flutter: Debug (Chrome)",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main.dart",
      "flutterMode": "debug",
      "deviceId": "chrome",
      "console": "terminal",
      "args": [
        "--web-renderer",
        "html"
      ]
    },
    {
      "name": "Flutter: Profile Mode",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main.dart",
      "flutterMode": "profile",
      "console": "terminal"
    },
    {
      "name": "Flutter: Release Mode",
      "request": "launch",
      "type": "dart",
      "program": "${workspaceFolder}/lib/main.dart",
      "flutterMode": "release",
      "console": "terminal"
    }
  ]
}
