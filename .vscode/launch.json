{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter-当前连接设备", // 自定义名称
      "request": "launch",
      "type": "dart", // Flutter/Dart 固定调试器类型
      "program": "${workspaceFolder}/lib/main.dart", // Flutter 入口必为 main.dart
      "flutterMode": "debug", // 调试模式：debug（可断点）、profile（性能分析）、release（无调试信息）
      "args": ["--dart-define", "ENV=dev"], // 传自定义环境变量（如区分开发/测试环境）
      "console": "terminal" // 显示Flutter日志
    }
  ]
}
